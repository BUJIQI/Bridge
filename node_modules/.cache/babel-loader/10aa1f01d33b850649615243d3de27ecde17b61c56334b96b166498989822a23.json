hen(response => {\r\n        this.competeOutcome = response.data;       \r\n      })\r\n      .catch(error => {\r\n        console.error('Error fetching data:', error);\r\n      });\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.no-report-message {\r\n  display: flex; \r\n  justify-content: center; \r\n  align-items: center; \r\n  height: calc(100vh - 100px); \r\n  font-size: 50px; \r\n  color: #333; \r\n  font-weight: bold; \r\n  text-align: center;\r\n}\r\n\r\n.panel {\r\n  margin-top: 20px;\r\n  border-radius: 8px;\r\n}\r\n\r\n.panel-heading {\r\n  background-color: #f7f7f9;\r\n  padding: 15px;\r\n}\r\n\r\n.panel-body {\r\n  background-color: #ebdcdc;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n}\r\n\r\n.data-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-top: 10px;\r\n}\r\n\r\n.data-table th,\r\n.data-table td {\r\n  border: 1px solid #ddd;\r\n  padding: 10px;\r\n  text-align: center;\r\n}\r\n\r\n.data-table th {\r\n  background-color: #c0392b; \r\n  color: #fff; \r\n}\r\n\r\n.data-table tr:nth-child(even) {\r\n  background-color: #e0e0e0; \r\n}\r\n\r\n.data-table tr:hover {\r\n  background-color: #ddd; \r\n}\r\n\r\n.data-table td {\r\n  color: #555;\r\n}\r\n</style>\r\n"],"mappings":"AA+BA,SAASA,YAAW,QAAS,cAAc;AAC3C,OAAOC,KAAI,MAAO,aAAa;AAE/B,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIH,YAAY,CAAC,CAAC;IAChC,MAAMI,QAAO,GAAID,SAAS,CAACC,QAAO;IAElC,OAAO;MACLA;IACF,CAAC;EAEH,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,CAAC,CAAC;MAClBC,YAAY,EAAE;QACZ,QAAQ,EAAE,KAAK;QACf,QAAQ,EAAE,KAAK;QACf,QAAQ,EAAE,GAAG;QACb,QAAQ,EAAE,QAAQ;QAClB,SAAS,EAAE,GAAG;QACd,SAAS,EAAE,KAAK;QAChB,SAAS,EAAE,GAAG;QACd,SAAS,EAAE,GAAG;QACd,UAAU,EAAE,GAAG;QACf,UAAU,EAAE,KAAK;QACjB,WAAW,EAAE,GAAG;QAChB,WAAW,EAAE,KAAK;QAClB,MAAM,EAAE,SAAS;QACjB,UAAU,EAAE,KAAK;QACjB,UAAU,EAAE,GAAG;QACf,SAAS,EAAE,MAAM;QACjB,QAAQ,EAAE,KAAK;QACf,QAAQ,EAAE;MACZ,CAAC;MACHC,iBAAiB,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,sBAAsBA,CAAA,EAAG;MACvB,OAAOC,MAAM,CAACC,WAAW,CAACD